{
	"compilerOptions": {
		"locale": "ru",
		"sourceMap": true,
		"lib": ["es6", "dom", "es2016", "es2017", "es2018", "es2019", "es2020"],
		"module": "commonjs",
		"strict": true,
		"newLine": "lf",
		"importHelpers": true,
		"esModuleInterop": true,
		"target": "es6",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"rootDir": "./src",
		"baseUrl": "./src",
		"outDir": "./dist/"
	},
	"include": ["src/**/*"]
}
